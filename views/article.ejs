<%- include('header'); -%>
    <div class="content">
        <h1>Selected articles</h1>
        <table id="article">
            <thead>
                <tr>
                    <th>Name</th>
                    <th scope="col">Reference</th>
                    <th scope="col">Category</th>
                    <th scope="col">Price</th>
                </tr>
            </thead>
            <tbody>
                    <tr>
                        <th scope="row">
                            <%= article.name %>
                        </th>
                        <td>
                            <%= article.reference %>
                        </td>
                        <td>
                            <%= article.category %>
                        </td>
                        <td>
                            <%= article.price %>AED
                        </td>
                    </tr>
            </tbody>
        </table>
    </div>
    <br>
    <%- include('footer'); -%>